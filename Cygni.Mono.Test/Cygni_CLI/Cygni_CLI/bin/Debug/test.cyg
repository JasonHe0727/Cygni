class person {
	def __INIT__(name, age){
		this.name = name
		this.age = age
	}
	def say(){
		printf('My name is {0}', this.name)
	}
}

class doctor : person {
	def __INIT__(name, age, salary){
		base.__INIT__(name, age)
		this.salary = salary
	}
	def mySalary(){
		printf('My salary is {0}', this.salary)
	}
}
d1 = doctor('Jim', 89, 1000)
d1.mySalary()
d1.say()
